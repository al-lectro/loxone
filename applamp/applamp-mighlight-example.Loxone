  }
  // Test bitwise if input 5 is changed (00010000)
  if (nEvents &amp; 0x10) {
    SetGroup(3, getinput(2));
  }
  // Test bitwise if input 6 is changed (00100000)
  if (nEvents &amp; 0x20) {
    SetGroup(4, getinput(3));
  }

  sleep(500);
}

